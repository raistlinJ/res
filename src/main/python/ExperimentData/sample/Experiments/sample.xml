<?xml version="1.0" encoding="utf-8"?>
<xml>
	<vbox-setup>
		<path-to-vboxmanage>VBoxManage</path-to-vboxmanage>
	</vbox-setup>
	<testbed-setup>
		<network-config>
			<ip-address>11.0.0.2</ip-address>
		</network-config>
		<vm-set>
			<base-groupname>sample</base-groupname>
			<num-clones>5</num-clones>
			<linked-clones>true</linked-clones>
			<clone-snapshots>true</clone-snapshots>
			<base-outname>true</base-outname>
			<vrdp-baseport>1001</vrdp-baseport>
			<vm>
				<name>defaultc</name>
				<vrdp-enabled>false</vrdp-enabled>
				<internalnet-basename>intnet</internalnet-basename>
			</vm>
			<vm>
				<name>defaulta</name>
				<vrdp-enabled>false</vrdp-enabled>
				<internalnet-basename>intnet</internalnet-basename>
			</vm>
			<vm>
				<name>defaultb</name>
				<vrdp-enabled>false</vrdp-enabled>
				<internalnet-basename>intnet</internalnet-basename>
			</vm>
			<material>
				<name>sample.txt</name>
			</material>
			<material>
				<name>sample2.txt</name>
			</material>
		</vm-set>
	</testbed-setup>
</xml>