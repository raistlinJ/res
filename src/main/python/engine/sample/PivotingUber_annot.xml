<xml>
  <vbox-setup>
    <path-to-vboxmanage>VBoxManage</path-to-vboxmanage>
  </vbox-setup>
  <testbed-setup>
    <network-config>
      <ip-address>11.0.0.2</ip-address>
    </network-config>
    <vm-set>
      <base-groupname>PivotingUber_annot</base-groupname>
      <num-clones>7</num-clones>
      <clone-snapshots>true</clone-snapshots>
      <linked-clones>true</linked-clones>
      <base-outname>601</base-outname>
      <vrdp-baseport>6011</vrdp-baseport>
      <vagrant-provision-path></vagrant-provision-path>
      <vm>
        <name>kali-2019.2-64bit</name>
        <vrdp-enabled>true</vrdp-enabled>
        <on-startup>
        <command seq=1></command>
        <copyfrom seq=2><copyfrom>
        <on-startup>
        <on-stop>
        <copyfrom seq=1></command>
        <command seq=2><copyfrom>
        </on-stop>
        <internalnet-basename>pivoting_intnet01</internalnet-basename>
      </vm>
      <vm>
        <name>TinyLinux_plus</name>
        <vrdp-enabled>false</vrdp-enabled>
        <internalnet-basename>pivoting_intnet02</internalnet-basename>
      </vm>
      <vm>
        <name>kali-debian-32-topo-pivot</name>
        <vrdp-enabled>false</vrdp-enabled>
        <internalnet-basename>pivoting_intnet01</internalnet-basename>
        <internalnet-basename>pivoting_intnet02</internalnet-basename>
      </vm>
      <material>
        <name>Acosta_CyberRIG_FY20_v2.pptx</name>
      </material>
      <material>
        <name>Pivoting_Traffic_Exercise.docx</name>
      </material>
      <material>
        <name>Pivoting_Traffic_Presentation.pptx</name>
      </material>
    </vm-set>
  </testbed-setup>
</xml>
